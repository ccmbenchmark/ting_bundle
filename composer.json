{
    "name": "ccmbenchmark/ting_bundle",
    "description" : "Symfony 2 bundle for ccmbenchmark/ting",
    "type": "library",
    "license": "Apache-2.0",
    "require": {
        "php": ">=5.4",
        "ccmbenchmark/ting": "dev-feature/FAS-18-integration-debugbar"
    },
    "autoload": {
        "psr-4" : {
            "CCMBenchmark\\TingBundle\\": "src/TingBundle/"
        }
    },
    "repositories": {
        "ting": {
            "type": "git",
            "url": "/var/www/ting/"
        }
    },
    "minimum-stability" : "dev",
    "prefer-stable": true
}

