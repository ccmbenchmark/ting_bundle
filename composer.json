{
    "name": "ccmbenchmark/ting_bundle",
    "description" : "Symfony 2 bundle for ccmbenchmark/ting",
    "type": "symfony-bundle",
    "license": "Apache-2.0",
    "require": {
        "php": ">=5.5",
        "ccmbenchmark/ting": "^3.2",
        "doctrine/doctrine-cache-bundle": "^1.3.0",
        "symfony/validator": "^3.2",
        "symfony/http-kernel": "^3.2",
        "symfony/config": "^3.2"
    },
    "require-dev": {
        "atoum/atoum": "^2.8",
        "atoum/stubs": "^2.2"
    },
    "autoload": {
        "psr-4" : {
            "CCMBenchmark\\TingBundle\\": "src/TingBundle/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\fixtures\\": "tests/fixtures/"
        }
    },
    "minimum-stability" : "stable"
}
